<?php

/**
 * @file
 * Functions to support theming in the mowa theme.
 */

use Drupal\Core\Template\Attribute;

/**
 * Implements hook_preprocess_HOOK() for node.html.twig
 */
function mowa_preprocess_node(&$variables) {
    // Add custom css class to Article tag
    $variables['attributes']['class'] = array_merge($variables['attributes']['class'], ['mowa-article']);
}